}
.col-lg-12{
  padding: 0;
  margin: 0;
}
a{
  color: #333333;
  cursor: pointer;
}
a:hover{
  text-decoration: none;
  color: #afafaf;
}
a:active{
  text-decoration: none;
  color: #afafaf;
}
header{
    text-align: center;
    line-height: 144px;
    width: 100%;
    height: 100px;
    background-color: pink;
    overflow: hidden;
}
.detailHead{
  //background-color: #EEEEEE;
  margin: 10%;
  >.detailImg{
    padding: 0;
    margin: 0;
    text-align: right;
    >img{
      width:100%;
    };
  }
  >.detail{
    padding: 0;
    margin: 0;
    >a{
      display: block;
      padding: 10px;
      background-color: #e56a69;
      color: white;
      position: relative;
    }
    >a::after{
      content: '>';
      font-family: '宋体';
      font-weight: 700;
      position:absolute;
      right: 1%;
    }
    >.arrivalTime{
      margin-top: 10px;
      padding: 10px;
      background-color: #e0e0e0;
      position: relative;
    }
    >form{
      div{
        margin: 15px 0;
      }
      label{
        cursor:pointer;
        margin: 5px 2px;
        input{
          display: none;
        }
        input:checked+span{
          border: 3px solid #343434;
          padding: 7px;
        }
        span{
          display: inline-block;
          border: 1px solid #c1c1c1;
          padding: 9px;
          >img{
            width: 100%;
          }
        }
        span:hover{
          border: 1px solid #000000;
        }
      }
      >.detailColor{
        margin-top: 20px;
        >label{
          >span{
            width: 40px;
            height: 40px;
          }
        }
      }
      >div{
        div{
          float: left;
          margin: 0 1%;
        }
        >.GoodsNumber {
          width: 79px;
          border: 2px solid #000000;
          display: inline-block;
          label {
            float: left;
            margin: 0;
            padding: 0;
            input {
              border: none;
              padding: 0;
              display: block;
              width: 50px;
              height: 50px;
              text-align: center;
              font-size: 20px;
            }
          }
          div {
            float: left;
            padding: 0;
            margin: 0;
            display: inline-block;
            width: 25px;
            height: 25px;
            border-bottom: 1px solid #000000;
            border-left: 1px solid #000000;
            position: relative;
            transition:all .3s;
            cursor: pointer;
          }
          div:hover {
            background-color: #000000;
          }
          div:nth-of-type(1):after {
            content: '+';
            line-height: 20px;
            margin-left: 5px;
            font-size: 25px;
            position: absolute;
            top: 0;
          }
          div:nth-of-type(2):after {
            content: '-';
            line-height: 20px;
            margin-left: 7.5px;
            font-size: 25px;
            position: absolute;
            top: 0;

          }
          div:nth-of-type(2) {
            border-bottom: none;
          }
          div:hover:after{
            color: white;
          }
        }
      }
       button{
         height: 54px;
         padding: 0 20px;
         margin: 0 10px;
         display: inline-block;
         border: 2px solid #000000;
         background-color: #FFFFFF;
         font-size: 18px;
       }
      a{
        height: 54px;
        padding: 0 20px;
        margin: 0 10px;
        line-height: 50px;
        display: inline-block;
        border: 2px solid #000000;
        background-color: #333;
        color: #FFFFFF;
        font-size: 18px;
        vertical-align: bottom;
      }
      a:hover{
         background-color: #000000;
         color: #FFFFFF;
    }
       button:nth-of-type(2){
        background-color: #3c3c3c;
        color: #FFFFFF;
      }
       button:nth-of-type(2):hover{
        background-color: #000000;
        color: #FFFFFF;
      }
    }
  }
}
