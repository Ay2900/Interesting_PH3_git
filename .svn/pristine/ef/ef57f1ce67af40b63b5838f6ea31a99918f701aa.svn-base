  z-index: 999999;
  font-size:18px;
  div{
    border-radius: 8px;
    background-color: #fff;
    position: absolute;
    top: 50%-10px;
    left: 50%-2px;
    //width: 100px;
    //height: 40px;
    padding: 40px 60px;
    span:nth-of-type(1){
      display: inline-block;
      padding: 10px;
      font-size: 12px;
      color: #FFFFFF;
      background-color: #69f091;
      border-radius:50%;
    }

  }

}



.onTop{
  display: none;
  width: 100%;
  background-color: #333333;
  position: fixed;
  top: 0;
  z-index: 999;
  >.container-fluid{
    height: 80px;
    line-height: 80px;
    font-size: 20px;
    margin:0 10% 0 10%;
    div{
      display: inline-block;
    }
    >div:nth-of-type(1){
      ul{
        li{
          color: #EEEEEE;
          display: inline-block;
          margin: 0 50px;
          position: relative;
          cursor: pointer;
        }
        li:nth-of-type(1){
          margin-left: 20px;
        }
        li:after{
          content: '|';
          position: absolute;
          right: -50px;
        }
        li:nth-last-of-type(1):after{
          content: '';
        }
      }
    }
    >div:nth-of-type(2){

      color: #EEEEEE;
      float: right;
      li{
        float: left;
        margin: 0 10px;
      }
      li:nth-of-type(1){
        >button{
          padding: 0 40px;
          line-height: 50px;
          background-color: #e56a69;
          margin: 15px 0;
          border: none;
        }
      }
      li:nth-of-type(2){
        >a{
          vertical-align: top;
          display: inline-block;
          padding: 0 40px;
          line-height: 48px;
          background-color: #333333;
          margin: 15px 0;
          color: #EEEEEE;
          border: 1px solid #eee;
          cursor: pointer;
          position: relative;
        }
        #gouwuche:after{
          content: attr(data-attr);
          text-align: center;
          font-size: 12px;
          color: #333333;
          line-height: 20px;
          height: 20px;
          width: 20px;
          background-color: rgba(255, 192, 203, 0.8);
          border-radius: 40%;
          position: absolute;
          top: 5px;
          left: 55px;
          display: inline-block;
        }
        span{
          font-size: 22px;
        }
      }
    }
  }
}
#nav-car:after{
  content: attr(data-attr);
  text-align: center;
  font-size: 12px;
  color: #333333;
  line-height: 20px;
  height: 20px;
  width: 20px;
  background-color: rgba(255, 192, 203, 0.8);
  border-radius: 40%;
  position: absolute;
  top: 5px;
  left: 28px;
  display: inline-block;

}

@media screen and (max-width: 1200px) {
  .onTop {
    display: none;
  }
}

.addCart{
  display: none;
  position: absolute;
  right: 7.5%;
  .container-fluid{
    margin: 1% 2% 0 2%;
  }
  width: 440px;
  font-size: 12px;
  background-color: #fff;
}
.addCart:after{
  content: '';
  width: 0;
  height: 0;
  position: absolute;
  left: 5rem;
  top: -10px;
  z-index: 2;
  border-left: transparent 10px solid;
  border-right: transparent 10px solid;
  border-bottom: #fff 10px solid;
}





