.collect{
  position: fixed;
  border: 1px solid rgba(51, 51, 51, 0.16);
  border-right: none;
  right: 0;
  bottom: 20%;
  width: 60px;
  //height: 160px;
  background-color: rgba(255, 255, 255, 0.81);
  text-align: center;
  li:nth-last-of-type(1){
    display: none;
  }
  li{
    margin-top: 10px;
    position: relative;
  }

  li:after{
    content: '_____';
    //position: absolute;
  }
  li:nth-of-type(2):after{
    content: none;
  }
  span{
    font-size: 28px;
    color: #6e6e6e;
    cursor: pointer;
  }
  span:hover{
    color: #333333;
  }
  h6{
    margin: 1%;
  }
}
.CollectionOfSuccess{
  display: none;
