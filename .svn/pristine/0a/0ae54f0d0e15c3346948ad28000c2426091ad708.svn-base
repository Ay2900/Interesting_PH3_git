body {
  padding-top: 50px;
}
section {
  padding-top: 20px;
  padding-bottom: 20px;
}
section .huabei_item {
  height: 30%;
  width: 30%;
  padding-top: 10px;
  padding-bottom: 10px;
  outline: 1px dashed #333;
  margin: 1.5%;
  font-size: 7px;
  text-align: center;
}
section .huabei_item:hover {
  cursor: pointer;
}
section .huabei_item.active {
  outline: 2px solid #333;
}
section .col-xs-12 {
  margin: 5px 0;
}
.SKU_list {
  margin-bottom: 10px;
  overflow: hidden;
}
.SKU_list .pull-left {
  float: left;
  width: 30%;
}
.SKU_list .pull-left img {
  height: 50px;
}
.SKU_list .pull-right {
  text-align: right;
  float: right;
  width: 70%;
  margin-top: 3px;
}
