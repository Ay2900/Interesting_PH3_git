const dao=require('../dao/shopping_dao');
const controller={
    //展示购物车
    showCar(req,resp){
        let user_id=[req.session.username];
        dao.getShopping_car(user_id)
            .then((data)=>{
                console.log(data);
                resp.render('TYH/shopping_car',{items:data})
            });
    }
};

module.exports=controller;

