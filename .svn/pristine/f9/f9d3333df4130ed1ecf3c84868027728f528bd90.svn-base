<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">


    <link rel="stylesheet" href="../../frame/bootstrap-3.3.7-dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="../../css/XY/productList.css">
    <link rel="stylesheet" href="../../css/nav-footer/nav.css">
    <link rel="stylesheet" href="../../fonts/header_footer/iconfont.css">
    <!--[if lt IE 9]>
    <script src="https://cdn.jsdelivr.net/npm/html5shiv@3.7.3/dist/html5shiv.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/respond.js@1.4.2/dest/respond.min.js"></script>
    <![endif]-->
</head>
<body>
<!--导航部分-->
<%-include('../common/nav.ejs')%>
<!--导航（完）-->



<section class="row">
    <div class="container commodity">
        <%for(let j=0;j<mydata.length;j++){%>
        <div class="col-lg-3">
            <div>
                <a href="/<%=mydata[j].SPU_code%>" class="xiangqing">
                    <img src="<%=mydata[j].SPU_head_img_path%>" alt="">
                    <div class="myHide">
                        <div class="miaosu">
                            <%=mydata[j].SPU_describe%>
                        </div>
                        <div class="dibei">
                            <p><%=mydata[j].SPU_name%></p>
                            <p>中国 | 北京</p>
                        </div>
                    </div>
                    <div class="myShow">
                        <h4>
                            <%=mydata[j].SPU_name%>
                        </h4>
                        <p class="money">
                            <%=mydata[j].sell_price%>
                        </p>
                        <div class="selectiveColor">
                            6色可选
                        </div>
                    </div>
                </a>
            </div>
        </div>
        <%}%>
    </div>

</section>
<!--页脚-->
<%-include('../common/footer.ejs')%>
<!--页脚（完）-->
<script src="../../frame/jquery-3.4.1.min.js"></script>
<script src="../../frame/bootstrap-3.3.7-dist/js/bootstrap.min.js"></script>
</body>
</html>