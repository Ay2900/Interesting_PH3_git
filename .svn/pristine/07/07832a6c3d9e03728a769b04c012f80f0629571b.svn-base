<script>
    function chknext() {
        if ($("#inputAccount").val() == "") {
            alert("账号不能为空！");
//            console.log($("#inputAccount").val())
            return false;
        }
        else{
            $.ajax({
                type:"post",
                url:"/chkexist.do",
                data:{
                    username:$("#inputAccount").val()

                },
                datatype:"json",
                success:function (data) {
                    console.log(data);
                    if(data.length>0){
                        $("#pwd1").css("display","none");
                        $("#pwd2").css("visibility","visible");

                    }
                    else {
                        alert("输入的账号不存在！");
                    }

                }
//                err:function (err) {
//                    alert("输入的账号不存在！");
//                    console.log()
//
//                }
            })
        }
//        else{
//            $("#pwd1").css("display","none");
//            $("#pwd2").css("visibility","visible");
//            console.log($("#inputAccount").val())
//
//        }

    }
    function chgpwd() {

            if ($("#inputpwd1").val() == "") {
                alert("密码不能为空！");
                return false;
            }
            if ($("#inputpwd2").val() == "") {
                alert("密码不能为空！");
                return false;
            }
            if($("#inputpwd1").val()==$("#inputpwd2").val()){
                var regz = /^[A-Za-z0-9_-]{3,}$/;
                if (!regz.test($("#inputpwd1").val())) {
                    alert("请输入输入3位以上英文或数字！");
                    return false;
                }
                else {
                    $.ajax({
                        type:"post",
                        url:"/chgpwd.do",
                        data:{
                            username:$("#inputAccount").val(),
                            password:$("#inputpwd1").val()

                        },
                        datatype:"json",
                        success:function (data) {
                            console.log(data);
                            if(data=="修改成功"){
               alert("修改成功！");
               location.href=('login.html')
                            }


                        }
                    })

                }

            }
            else if($("#inputpwd1").val()!=$("#inputpwd2").val()){
                alert("前后密码不一致！");
                return false;
            }
    }



</script>
